# n8n Configuration (example) â€” copy to .env and fill with your values

# Cloudflare tunnel token (do not commit real secrets)
TUNNEL_TOKEN=YOUR_TUNNEL_TOKEN

# TLS cert/key filenames placed in repo root
CERT_FILE=n8n.example.com.pem
KEY_FILE=n8n.example.com.key

# Hostname that matches your cert
N8N_HOST=n8n.example.com

# Runner auth token (shared between n8n and runners)
N8N_RUNNERS_AUTH_TOKEN=replace_with_strong_random_hex

# Trust proxies (default trusts all; set to stricter CIDRs if desired)
N8N_TRUSTED_PROXIES=*

# Runner settings (optional overrides)
N8N_RUNNERS_MAX_CONCURRENCY=3
N8N_RUNNERS_MAX_CONCURRENCY_PY=3
N8N_RUNNERS_AUTO_SHUTDOWN_TIMEOUT=300
N8N_RUNNERS_INSTANCE_URL=https://n8n.example.com

# Trust proxies (set to your docker subnets/loopback; do not leave wildcard in production)
N8N_TRUSTED_PROXIES=127.0.0.1/8,172.30.10.0/24,172.30.11.0/24

# Optional: network subnets
# N8N_INTERNAL_SUBNET=172.30.10.0/24
# N8N_EGRESS_SUBNET=172.30.11.0/24

# Optional: custom cloudflared config path
# CLOUDFLARED_CONFIG_PATH=./cloudflared-config.yml
